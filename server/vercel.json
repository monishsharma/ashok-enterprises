{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],

    "rewrites": [
        { "source": "/(.*)", "destination": "/someroute" }
    ],
    "crons": [
        {
            "path": "/cron",
            "schedule": "0 0 * * *"
        }
    ]
}
